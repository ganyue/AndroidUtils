<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <!--<item android:id="@android:id/background"-->
        <!--android:drawable="@drawable/img_seekbar_bg3"/>-->

    <item android:id="@android:id/background"
        android:drawable="@drawable/seekbar_bg"/>

    <!--<item android:id="@android:id/progress"-->
        <!--android:drawable="@drawable/img_seekbar_bg1"/>-->

    <!--<item android:id="@android:id/secondaryProgress"-->
        <!--android:drawable="@drawable/img_seekbar_bg2"/>-->

    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="5dp" />
                <solid android:color="#6b3925"/>
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="#dacec9"
                    android:endColor="#dacec9"
                    android:angle="270.0"
                    android:centerY="0.75"
                    android:centerColor="#dacec9" />
            </shape>
        </clip>
    </item>

</layer-list>